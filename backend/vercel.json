{
  "version": 2,
  "builds": [
    {
      "src": "run.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "run.py"
    }
  ],
  "env": {
    "SECRET_KEY": "@secret-key",
    "ALGORITHM": "HS256",
    "ACCESS_TOKEN_EXPIRE_MINUTES": "30",
    "ENVIRONMENT": "production",
    "HOST": "0.0.0.0",
    "PORT": "8000",
    "FRONTEND_URL": "https://concurso-ai-orchestrated.vercel.app"
  }
}
