# Configurações para Deploy na Vercel
# Copie este arquivo para .env.local e ajuste os valores

# API Configuration
NEXT_PUBLIC_API_URL=https://your-backend-api.vercel.app
NEXT_PUBLIC_APP_NAME=Concurso AI

# Vercel Configuration
VERCEL_URL=your-app-name.vercel.app
NEXT_PUBLIC_VERCEL_URL=https://your-app-name.vercel.app

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_DEBUG=false

# Performance
NEXT_PUBLIC_ENABLE_PWA=true
NEXT_PUBLIC_ENABLE_OFFLINE=false

# Security
NEXT_PUBLIC_ENABLE_CSP=true
